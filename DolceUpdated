-- Scripts:

local function ZCNRUD_fake_script() -- TextButton.Destroy 
	local script = Instance.new('LocalScript', TextButton)

	script.Parent.MouseButton1Up:Connect(function()
		local Sound = Instance.new("Sound", script.Parent)
		Sound.SoundId = "rbxassetid://5801257793"
		Sound:Play()
		local Gui = script.Parent.Parent.Parent.Parent
		Gui:Destroy()
	end)
end
coroutine.wrap(ZCNRUD_fake_script)()
local function THEHKRP_fake_script() -- StairsBreaker.StairBreaker 
	local script = Instance.new('LocalScript', StairsBreaker)

	script.Parent.MouseButton1Up:Connect(function()
		
		local SoundClick = Instance.new("Sound", script.Parent)
		SoundClick.SoundId = "rbxassetid://452267918"
		SoundClick:Play()
		
		local Player = game.Players.LocalPlayer
		local Part = Instance.new("Part", game.Workspace)
		local Stairs = game.Workspace.Stairs:GetChildren()
		local HumanoidRootPart = Player.Character.HumanoidRootPart
		local InputText = script.Parent.Parent.Parent.Input
		
		Part.Anchored = true
		
		Part.Position = Vector3.new(81.015152, 146.53299, -250.052811, -0.978149116, 0.16117464, -0.131327629, 0.135372892, 0.973167658, 0.186061695, 0.157792225, 0.164217904, -0.973721802)
		
		Part.CanCollide = false
		
		Player.Character.Humanoid.WalkSpeed = 0
		
		wait(3)
		
		HumanoidRootPart.CFrame = Part.CFrame
		
		for i,StairsE in pairs(Stairs) do
			StairsE.Position = Vector3.new(81.015152, 146.53299, -250.052811, -0.978149116, 0.16117464, -0.131327629, 0.135372892, 0.973167658, 0.186061695, 0.157792225, 0.164217904, -0.973721802)
			StairsE.CanCollide = false
		end
		
		wait(2)
		
		InputText.Text = "Injected!"
		wait(3)
		InputText.Text = " "
		
	end)
end
coroutine.wrap(THEHKRP_fake_script)()
local function PHCYP_fake_script() -- LocalHolder.DragAbleGui 
	local script = Instance.new('LocalScript', LocalHolder)

	local Gui = script.Parent
	Gui.Draggable = true
	Gui.Active = true
end
coroutine.wrap(PHCYP_fake_script)()
local function KDPVT_fake_script() -- ReJoin.ReJoin 
	local script = Instance.new('LocalScript', ReJoin)

	local button = script.Parent
	local TeleportService = game:GetService("TeleportService")
	local placeId = 8362263552
	
	button.MouseButton1Click:Connect(function()
		TeleportService:Teleport(placeId, game.Players.LocalPlayer)
	end)
end
coroutine.wrap(KDPVT_fake_script)()
local function FQDSG_fake_script() -- LocalHolder.DestroyTheUseless 
	local script = Instance.new('LocalScript', LocalHolder)

	local Dumbass = game.Workspace.Stairs.ss
	
	Dumbass:Destroy()
end
coroutine.wrap(FQDSG_fake_script)()
